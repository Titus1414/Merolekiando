    <link rel="stylesheet" href="~/dash/vendors/gijgo/gijgo.min.css" />
    <link rel="stylesheet" href="~/dash/vendors/font_awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/dash/vendors/tagsinput/tagsinput.css" />
