<body class="crm_body_bg">
    <nav class="sidebar">
        <div class="logo d-flex justify-content-between">
            <a href="index.html"><img src="img/logo.png" alt=""></a>
            <div class="sidebar_close_icon d-lg-none">
                <i class="ti-close"></i>
            </div>
        </div>
        <ul id="sidebar_menu">
            <li class="mm-active">
                <a asp-controller="Home" asp-action="Index" aria-expanded="false">
                    <img src="img/menu-icon/1.svg" alt="">
                    <span>Dashboard</span>
                </a>
            </li>
            <li class="">
                <a asp-controller="Category" asp-action="Index" aria-expanded="false">
                    <img src="img/menu-icon/2.svg" alt="">
                    <span>Categories</span>
                </a>
            </li>
            <li class="">
                <a asp-controller="User" asp-action="Verification" aria-expanded="false">
                    <img src="img/menu-icon/3.svg" alt="">
                    <span>Verification</span>
                </a>
            </li>
            <li class="">
                <a asp-controller="User" asp-action="Subscription" aria-expanded="false">
                    <img src="img/menu-icon/4.svg" alt="">
                    <span>Subscriptions</span>
                </a>
            </li>
            <li class="">
                <a asp-controller="Province" asp-action="Index" href="#" aria-expanded="false">
                    <img src="img/menu-icon/5.svg" alt="">
                    <span>Provinces</span>
                </a>
                @*<ul>
                    <li><a href="chart_box_1.html">Chart Boxes 1</a></li>
                    <li><a href="profilebox.html">Profile Box</a></li>
                </ul>*@
            </li>
            <li class="">
                <a class="has-arrow" href="#" aria-expanded="false">
                    <img src="img/menu-icon/6.svg" alt="">
                    <span>Forms</span>
                </a>
                <ul>
                    <li>
                        <a href="#">Elements</a>
                        <ul>
                            <li><a href="data_table.html">Data Tables</a></li>
                            <li><a href="bootstrap_table.html">Grid Tables</a></li>
                            <li><a href="datepicker.html">Date Picker</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">Widgets</a>
                        <ul>
                            <li><a href="Input_Selects.html">Input Selects</a></li>
                            <li><a href="Input_Mask.html">Input Mask</a></li>
                        </ul>
                    </li>
                </ul>
            </li>
            <li class="">
                <a class="has-arrow" href="#" aria-expanded="false">
                    <img src="img/menu-icon/7.svg" alt="">
                    <span>Charts</span>
                </a>
                <ul>
                    <li><a href="chartjs.html">ChartJS</a></li>
                    <li><a href="apex_chart.html">Apex Charts</a></li>
                    <li><a href="chart_sparkline.html">chart sparkline</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <section class="main_content dashboard_part">
        <div class="container-fluid g-0">
            <div class="row">
                <div class="col-lg-12 p-0">
                    <div class="header_iner d-flex justify-content-between align-items-center">
                        <div class="sidebar_icon d-lg-none">
                            <i class="ti-menu"></i>
                        </div>
                        <div class="serach_field-area">
                            <div class="search_inner">
                                <form action="#">
                                    <div class="search_field">
                                        <input type="text" placeholder="Search here...">
                                    </div>
                                    <button type="submit"> <img src="img/icon/icon_search.svg" alt=""> </button>
                                </form>
                            </div>
                        </div>
                        <div class="header_right d-flex justify-content-between align-items-center">
                            <div class="header_notification_warp d-flex align-items-center">
                                <li>
                                    <a href="#"> <img src="img/icon/bell.svg" alt=""> </a>
                                </li>
                                <li>
                                    <a href="#"> <img src="img/icon/msg.svg" alt=""> </a>
                                </li>
                            </div>
                            <div class="profile_info">
                                <img src="img/client_img.png" alt="#">
                                <div class="profile_info_iner">
                                    <p>Welcome Admin!</p>
                                    <h5>Travor James</h5>
                                    <div class="profile_info_details">
                                        <a href="#">My Profile <i class="ti-user"></i></a>
                                        <a href="#">Settings <i class="ti-settings"></i></a>
                                        <a asp-controller="Home" asp-action="Logout">Log Out <i class="ti-shift-left"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="main_content_iner ">
            @RenderBody()
        </div>
        <div class="footer_part">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-sm-12">
                        <div class="footer_iner text-center">
                            <p>2020 © Influence - Designed by<a href="#"> Dashboard</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="~/dash/js/jquery1-3.4.1.min.js"></script>
    <script src="~/dash/js/popper1.min.js"></script>
    <script src="~/dash/js/bootstrap1.min.js"></script>
    <script src="~/dash/js/metisMenu.js"></script>
    <script src="~/dash/vendors/count_up/jquery.waypoints.min.js"></script>
    <script src="~/dash/vendors/chartlist/Chart.min.js"></script>
    <script src="~/dash/vendors/count_up/jquery.counterup.min.js"></script>
    <script src="~/dash/vendors/swiper_slider/js/swiper.min.js"></script>
    <script src="~/dash/vendors/niceselect/js/jquery.nice-select.min.js"></script>
    <script src="~/dash/vendors/owl_carousel/js/owl.carousel.min.js"></script>
    <script src="~/dash/vendors/gijgo/gijgo.min.js"></script>
