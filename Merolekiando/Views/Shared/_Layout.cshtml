    <script src="~/dash/vendors/datatable/js/buttons.print.min.js"></script>
    <script src="~/dash/js/chart.min.js"></script>
    <script src="~/dash/vendors/progressbar/jquery.barfiller.js"></script>
    <script src="~/dash/vendors/tagsinput/tagsinput.js"></script>
    <script src="~/dash/vendors/text_editor/summernote-bs4.js"></script>
